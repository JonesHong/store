{
  "name": "mycena-store",
  "version": "1.2.22-beta.24",
  "description": "For mirgation",
  "author": {
    "name": "JonesHong",
    "email": "latte831104@gmail.com"
  },
  "contributors": [
    {
      "name": "Heng-Shiou Sheu",
      "email": "hengshiousheu@gmail.com"
    },
    {
      "name": "YouJun Huang",
      "email": "eric444555@gmail.com"
    }
  ],
  "main": "build/main/index.js",
  "engines": {
    "node": "*"
  },
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/JonesHong/store",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "run-p build:*",
    "build:main": "ttsc -p tsconfig.json",
    "build:module": "ttsc -p tsconfig.module.json",
    "build:watch": "ttsc -p tsconfig.json -w",
    "note:publish": "npm run build:main && npm publish --access public",
    "note:unpublish": "npm unpublish <package_name>@<package_version>",
    "note:deprecate": "npm deprecate <package>@<version> '<message>'",
    "note:unpublish-example": "npm unpublish mycena-store@1.2.20-beta.26",
    "note:deprecate-example": "npm deprecate mycena-store@1.2.22-beta.2 'this package has been deprecated'",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:spelling": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:unit": "nyc --silent ava",
    "check-cli": "run-s test diff-integration-tests check-integration-tests",
    "check-integration-tests": "run-s check-integration-test:*",
    "diff-integration-tests": "mkdir -p diff && rm -rf diff/test && cp -r test diff/test && rm -rf diff/test/test-*/.git && cd diff && git init --quiet && git add -A && git commit --quiet --no-verify --allow-empty -m 'WIP' && echo '\\n\\nCommitted most recent integration test output in the \"diff\" directory. Review the changes with \"cd diff && git diff HEAD\" or your preferred git diff viewer.'",
    "watch:test": "nyc --silent ava --watch",
    "cov": "run-s build test:unit cov:html cov:lcov && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:lcov": "nyc report --reporter=lcov",
    "cov:send": "run-s cov:lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset-hard": "git clean -dfx && git reset --hard && npm i",
    "prepare-release": "run-s reset-hard test cov:check doc:html version doc:publish"
  },
  "dependencies": {
    "ansi-html": "^0.0.9",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "inversify": "^6.0.1",
    "lodash": "^4.17.21",
    "lowdb": "^1.0.0",
    "luxon": "^3.0.3",
    "nanoid": "^3.3.4",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "strip-ansi": "^6.0.1",
    "tslib": "^2.4.0",
    "ttypescript": "^1.5.13",
    "typescript-transform-paths": "^3.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^3.0.0",
    "@types/node": "^18.7.16",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "ava": "^4.3.3",
    "codecov": "^3.5.0",
    "cspell": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^7.0.1",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "ts-node": "^10.8.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.7.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "build/main/"
      }
    },
    "files": [
      "!build/module/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}